{
	"printWidth": 80,
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "none",
	"bracketSpacing": false,
	"jsxBracketSameLine": false,
	"arrowParens": "avoid",
	"parser": "babylon",
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "preserve",
	"overrides": [
		{
			"files": "*.json",
			"options": {"parser": "json"}
		}
	]
}
